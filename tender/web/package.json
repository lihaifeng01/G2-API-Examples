{
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,less}\"",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "sync:online": "sh bin/sync-online.sh master && sh bin/publish-online.sh",
    "sync:test": "sh bin/sync-online.sh test && sh bin/publish-test.sh"
  },
  "dependencies": {
    "@umijs/plugins": "^4.4.9",
    "husky": "^8.0.3",
    "js-sha1": "^0.7.0",
    "umi": "^4.4.9",
    "wasm-feature-detect": "^1.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.62.0",
    "@umijs/lint": "^4.4.9",
    "ahooks": "^3.8.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eventemitter3": "^5.0.1",
    "lint-staged": "^15.5.1",
    "nertc-web-sdk": "^5.8.16",
    "stylelint": "^14.16.1",
    "typescript": "^5.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint"
    ]
  }
}
